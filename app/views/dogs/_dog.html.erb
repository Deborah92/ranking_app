<%= content_tag_for :tr, dog do %>
  <td><%= link_to image_tag(dog.image, class: "imageDog img-rounded"), dog_path(dog) %></td>
  <td><%= link_to dog.name, dog_path(dog) %></td>
  <td><% if !dog.user.nil? %>
        <%= link_to dog.user.email, user_path(dog.user) %>
      <% end %>
  </td>
  <td><%= link_to("Show", dog) %></td>
  <td><%= link_to("Edit", edit_dog_path(dog)) %></td>
  <td><%= link_to("Delete", dog, method: :delete, data: {confirm: 'Are you sure you want to delete this dog'} ) %></td>
<% end %>