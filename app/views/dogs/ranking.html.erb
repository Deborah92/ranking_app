<h2 class="title">Dogs' Ranking</h2>

<table class="table table-hover table-striped">
  <tr>
    <th>Points</th>
    <th>Photo</th>
    <th>Name</th>
    <th>Sex</th>
  </tr>
  <% @points.each do |k,v| %>
    <tr>
        <td><%= v[0] %></td>
        <td><%= link_to image_tag(v[1], class: "imageDog img-rounded"), dog_path(v[3]) %></td>
        <td><%= link_to k, dog_path(v[3]) %></td>
        <td><%= v[2] %></td>
    </tr>
  <% end %>
</table>

