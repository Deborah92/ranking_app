<%= content_tag_for :tr, result do %>
    <td><%= link_to result.exhibition.try(:name), exhibition_path(result.exhibition) %></td>
    <td><%= result.award.try(:award) %></td>
    <td><%= link_to result.dog.try(:name), dog_path(result.dog) %></td>
    <td><%= result.status %></td>
    <td><%= link_to("Edit", edit_admin_result_path(result)) %></td>
    <td><%= link_to("Delete", result, method: :delete, data: {confirm: 'Are you sure you want to delete this result'} ) %></td>
<% end %>