<%= content_tag_for :tr, user do %>
    <td>
      <% if user.image.present? %>
        <%= link_to image_tag(user.image_url, class: "imgUser img-rounded"), admin_user_path(user) %>
      <% end %>
    </td>
    <td><% if !user.cahoot? %>
            <%= link_to user, admin_user_path(user) %>
        <% else %>
            <% u=user.to_s+"(Cahoot)" %>
            <%= link_to u, admin_user_path(user) %>
        <% end %>
    </td>
    <td><%= link_to("Show", admin_user_path(user)) %></td>
    <td><%= link_to("Edit", edit_admin_user_path(user)) %></td>
    <td><%= link_to("Delete", admin_user_path(user), method: :delete, data: {confirm: 'Are you sure you want to delete this user'} ) %></td>
<% end %>